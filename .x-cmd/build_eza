# shellcheck shell=dash

(
    x env try rust
    git clone https://github.com/eza-community/eza.git
    x cd eza
    cargo install --path .
)

(
    git clone https://github.com/x-cmd/pkg.git
    x cd pkg
    x rmrf "./doc/"
    x cp "$HOME/.cargo/bin/eza" "./doc/"
    git add .
    git commit -m ...
    git push origin main
)