# shellcheck shell=dash

set -o errexit

test_single_main()(
    x cd .tmp/x-cmd-pkg && \
    xws install "$___X_CMD_PKG_CANDADITE" && \
    x pkg _smoke "$___X_CMD_PKG_CANDADITE" "${___X_CMD_PKG_CANDADITE_VERSION}" || return
)

x cd "$(x wsroot)"
xws prepare || return 1
test_single_main "${___X_CMD_PKG_VERSION:-v0.0.2}" || return
