# shellcheck shell=dash

test_xbin_main()(
    x cd .tmp/x-cmd-pkg && \
    x ws bundle && x rmrf "$___X_CMD_PKG_RAW_PATH" && \
    x ws test_smoke "$___X_CMD_WEBSRC_REGION" && \
    x cp -rf doc/* ../../doc/
)

x cd "$(x wsroot)"
x ws prepare || return 1
test_xbin_main "${___X_CMD_PKG_VERSION:-v0.0.4}"